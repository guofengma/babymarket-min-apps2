<!--地址信息-->
<template name="address-info">
  <view class="address-view" bindtap="addressInfo" data-index="{{index}}">
    <view class="address-row">
      <view class="address-name">{{LXR}}</view>
      <view class="address-mobile">{{LXFS}}</view>
    </view>
    <view class="address-detail">{{DZ}}</view>
  </view>
</template>
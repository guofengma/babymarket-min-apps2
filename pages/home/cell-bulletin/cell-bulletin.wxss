.cell-bulletin {
  display: flex;
  background-color: white;
  align-items: center;
}

.cell-bulletin .title {
  color: #454950;
  font-size: 36rpx;
  padding: 20rpx 30rpx 20rpx 30rpx;
  border-right: 1px solid #f1f3f4;
}

.cell-bulletin .content {
  flex-grow: 1;
  color: #5e6064;
  font-size: 26rpx;
  padding: 0rpx 30rpx 0rpx 30rpx;
}

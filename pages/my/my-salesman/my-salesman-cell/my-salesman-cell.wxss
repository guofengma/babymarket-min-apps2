.my-salesman-cell {
  display: flex;
  width: 100%;
  height: 176rpx;
  align-items: center;
  background: white;
}

.avatar {
  width: 120rpx;
  height: 120rpx;
  margin-left: 22rpx;
  flex-shrink: 0;
  border-radius: 60rpx;
}

.name-field {
  margin-left: 28rpx;
  display: flex;
  flex-direction: column;
  width: 380rpx;
  margin-right: 20rpx;
}

.name {
  font-size: 32rpx;
  color: #1a1c1f;
}

.nick {
  font-size: 26rpx;
  color: #b0b1b3;
  margin-top: 10rpx;
}

.right-view-flex{
    display: flex;
    justify-content: flex-end;
    font-size: 30rpx;
    color: #7d7f81;
    margin-right: 20rpx;
    width: 160rpx;
}

.number{
    color: #af8327;
}

.arrow{
  margin-right: 22rpx;
}
.line{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom:15rpx;
}
.content{
  width: 375rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: right;
  padding-right:30rpx;
}
.infos{
  padding-left: 30rpx;
  font-size: 26rpx;
  color: #979797;
  border-bottom: 1rpx solid #dcdcdc;
  background: #fff;
}
.infos>.line:first-child{
  padding-top: 15rpx;
  border-top: 1rpx solid #dcdcdc;
}
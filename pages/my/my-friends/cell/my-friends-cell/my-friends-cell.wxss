.my-friends-cell {
  display: flex;
  width: 100%;
  height: 176rpx;
  align-items: center;
  background: white;
}

.avatar {
  width: 120rpx;
  height: 120rpx;
  margin-left: 22rpx;
  flex-shrink: 0;
  border-radius: 60rpx;
}

.name-field {
  margin-left: 28rpx;
  display: flex;
  flex-direction: column;
}

.name {
  font-size: 32rpx;
  color: #1a1c1f;
}

.nick {
  font-size: 26rpx;
  color: #b0b1b3;
  margin-top: 10rpx;
}

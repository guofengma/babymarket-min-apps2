<!--pages/my/my-award/filter-result-view/cell/filter-result-cell.wxml-->
<view class="filter-result-cell">
    <image class="img" src="{{img}}"/>
    <view class="content">
        <view class="top">
            <view class="title">{{name}}</view>
            <view class="price">+￥{{price}}</view>
        </view>

        <view class="bottom">
            <view class="order-no">订单号{{orderno}}</view>
            <view class="time">{{time}}</view>
        </view>
    </view>
</view>

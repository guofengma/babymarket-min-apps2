<!--pages/my/my-award/filter-result-view/filter-result-view.wxml-->
<view>
    <my-award-nav-header title="推荐奖励" imgsrc="/res/img/my/my-award-active-icon.png" value="{{value}}"></my-award-nav-header>
    <view class="sep-line"></view>
    <view style="height:42rpx;width:100%;background-color:#F6F6F6"></view>
    <block wx:if="{{items.length > 0}}">
        <view class="sep-line"></view>
    </block>
    <block wx:for="{{items}}" wx:key="">
        <filter-result-cell
                img="{{item.BuyerImgId}}"
                name="{{item.Title}}"
                price="{{item.Commission}}"
                time="{{item.ConfirmDate}}"
                orderno="{{item.OrderNo}}"
        />
    </block>
</view>
